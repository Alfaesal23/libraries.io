# frozen_string_literal: true

version_attrs = %w[published_at original_license spdx_expression status].freeze

results = @versions.limit(@max_results).map do |version|
  version
    .attributes
    .slice(*version_attrs)
    .merge(
      {
        coordinate: Coordinate.generate(version.project, version.number),
      }
    )
end

{
  results: results,
  more: @versions.count > @max_results,
}
